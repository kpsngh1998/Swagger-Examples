{
  "swagger": "2.0",
  "info": {
    "version": "3.0.0",
    "title": "BoxAPI",
    "description": "These are the various BOX REST APIs"
  },
  "host": "api.box.com",
  "basePath": "/2.0/folders",
  "schemes": [
    "http"
  ],
  "paths": {
    "/{id}": {
      "get": {
        "operationId": "getFolders",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "type": "string",
            "required": true
          },
          {
            "in": "header",
            "name": "Content-Type",
            "default": "application/json",
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "successfully searched"
          }
        }
      }
    },
    "/": {
      "post": {
        "operationId": "createFolder",
        "parameters": [
          {
            "in": "body",
            "name": "requestBody",
            "schema": {
              "type": "object",
              "required": [
                "id"
              ],
              "properties": {
                "name": {
                  "type": "string"
                },
                "parent": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          {
            "in": "header",
            "name": "Content-Type",
            "default": "application/json",
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "successfully created"
          }
        }
      }
    }
  }
}